{

   "background": {
     //"scripts": ["background.js"],
      //"persistent": true
      "page": "background.html",
      "persistent": false
   },
   "browser_action": {
      "default_icon": "img/logo_32.png",
      "default_popup": "popup.html",
      "default_title": "Atomic Bookmarks 3"
   },
   "description": "Atomic Bookmarks 3",
   "default_locale": "en",
   "content_security_policy": "script-src 'self' 'unsafe-eval'  https://ssl.google-analytics.com; object-src 'self'",
   "name": "Atomic Bookmarks 3",
   "icons": {
      "16": "img/icon-16.png",
      "24": "img/icon-24.png",
      "48": "img/icon-48.png",
      "128": "img/icon-128.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "40",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "bookmarks", "chrome://favicon/", "http://*/*", "https://*/*", "tabs" ],
   "short_name": "Atomic Bookmarks 3",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.0.0"
}
